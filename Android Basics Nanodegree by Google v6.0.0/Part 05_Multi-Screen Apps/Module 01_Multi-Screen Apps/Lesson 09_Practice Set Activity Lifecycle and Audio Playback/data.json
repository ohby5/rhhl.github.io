              "question": {
                "prompt": "In the onCreate method of a given Activity, what does the call to  super.onCreate() do?",
                "answers": [
                  {
                    "id": "a1503338329651",
                    "text": "It plays audio from the super class.",
                    "is_correct": false
                  },
                  {
                    "id": "a1503338335414",
                    "text": "It returns null from the super class.",
                    "is_correct": false
                  },
                  {
                    "id": "a1503338336127",
                    "text": "It calls the super class constructor and allows the activity to enter the \"Created\" state.",
                    "is_correct": true
                  },
                  {
                    "id": "a1503338337179",
                    "text": "It creates a thread from the super class.",
                    "is_correct": false
                  }
                ]
              }
            }
