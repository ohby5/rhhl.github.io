                "unstructured": null
              },
              "question": {
                "prompt": "What's wrong with the code below?\n\n\t@Override\n\tprotected void onCreate(Bundle savedInstanceState) {\n\t        setContentView(R.layout.main);\n                MediaPlayer mediaPlayer = MediaPlayer.create(context, R.sound);\n                mediaPlayer.start();\n\t\tToast.makeText(getApplicationContext(), \"onStop called\", Toast.LENGTH_LONG).show();\n\t}\n",
                "answers": [
                  {
                    "id": "a1504902809774",
                    "text": "It didn't call super.onCreate(savedInstanceState) first",
                    "is_correct": true
                  },
                  {
                    "id": "a1504905546669",
                    "text": "It didn't call mediaPlayer.prepare()",
                    "is_correct": false
                  },
                  {
                    "id": "a1504905547486",
                    "text": "You can't create a MediaPlayer in the onCreate method",
                    "is_correct": false
                  },
                  {
                    "id": "a1504905548531",
                    "text": "There is nothing wrong.",
                    "is_correct": false
                  }
                ]
              }
            }
          ]
